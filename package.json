    "build": "nest build && cp -r prisma dist && copyfiles -u 1 src/**/*.csv src/**/*.mo src/**/**/*.mo src/**/*.py src/**/**/*.py src/**/*.wasm src/**/**/*.wasm src/**/**/**/*.wasm src/**/**/**/**/*.wasm dist/",
